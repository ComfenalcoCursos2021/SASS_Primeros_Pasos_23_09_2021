doctype html
html(lang="sp")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title SASS Primeros Pasos 23 09 2021
        link(rel="stylesheet", href="style.css")
        script(src="main.js")
    body 
        mixin cajas(nombre)
            -var n = 1;
            while n <= 100
                div(class="box-"+n) #{nombre} #{n++}
        .box
            +cajas("Caja")